{
  "name": "@wheel/design-system",
  "version": "0.0.1",
  "description": "THE WHEEL design system monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all",
    "dev": "nx run-many --target=dev --all --parallel",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev:storybook\" \"npm run dev:packages\"",
    "dev:server": "nodemon dev-server.js",
    "dev:storybook": "npx storybook dev -p 6008",
    "dev:packages": "nx run-many --target=dev --all --parallel",
    "storybook": "npx storybook dev -p 6008",
    "build-storybook": "npx storybook build",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "type-check": "nx run-many --target=type-check --all",
    "format": "prettier --write \"packages/**/*.{ts,tsx,js,jsx,json,css,md}\" \"*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"packages/**/*.{ts,tsx,js,jsx,json,css,md}\" \"*.{ts,tsx,js,jsx,json,css,md}\"",
    "validate": "npm run lint && npm run type-check && npm run test && npm run build",
    "clean": "nx run-many --target=clean --all && rm -rf node_modules dist",
    "reset": "npm run clean && rm -rf node_modules && npm install",
    "health-check": "node scripts/health-check.js",
    "env:setup": "node scripts/env-setup.js",
    "performance": "node scripts/performance-monitor.js",
    "debug": "node scripts/debug-tools.js",
    "build:analyze": "npm run build && npm run performance",
    "build:optimized": "node scripts/build-optimizer.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "install-packages": "npm install"
  },
  "devDependencies": {
    "@angular-devkit/core": "^16.2.0",
    "@angular-devkit/schematics": "^16.2.0",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.27.1",
    "@nx/devkit": "^16.10.0",
    "@nx/storybook": "^21.2.2",
    "@nx/workspace": "^16.10.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-controls": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "depcheck": "^1.4.7",
    "dotenv": "^16.4.5",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.3",
    "nx": "^16.10.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "socket.io": "^4.7.4",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.1",
    "terser": "^5.24.0",
